{
    "name": "codebyflame/thumbnails",
    "description": "Generates social media thumbnail images based on the title of a collection item",
    "version": "1.0.1",
    "type": "statamic-addon",
    "keywords": [
        "Statamic",
        "opengraph",
        "twitter",
        "facebook",
        "thumbnails",
        "CodeByFlame"
    ],
    "autoload": {
        "psr-4": {
            "CodeByFlame\\Thumbnails\\": "src"
        }
    },
    "authors": [
        {
            "name": "Flame",
            "email": "asko@hey.com"
        }
    ],
    "support": {
        "email": "asko@hey.com"
    },
    "require": {
        "php": ">=7.2.0",
        "ext-fileinfo": "*",
        "ext-gd": "*"
    },
    "extra": {
        "statamic": {
            "name": "Thumbnails",
            "description": "Generates social media thumbnail images based on the title of a collection item"
        },
        "laravel": {
            "providers": [
                "CodeByFlame\\Thumbnails\\ServiceProvider"
            ]
        }
    }
}
